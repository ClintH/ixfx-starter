{
  "env": {
    "es2024": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "unicorn"
  ],
  "extends": "plugin:unicorn/all",
  "rules": {
    "quotes": [
      "error",
      "backtick"
    ],
    "unicorn/consistent-destructuring": "off"
  }
}